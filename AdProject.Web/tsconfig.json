{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": [ "dom", "es5", "es2015" ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "compileOnSave": true,
  "typeAcquisition": { "enable": true }
}
